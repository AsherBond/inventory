                Magento\InventoryReservationCli\Model\SaleableQuantityInconsistency\FilterCompleteOrders\Proxy
            </argument>
            <argument name="filterIncompleteOrders" xsi:type="object">
                Magento\InventoryReservationCli\Model\SaleableQuantityInconsistency\FilterIncompleteOrders\Proxy
            </argument>
        </arguments>
    </type>
</config>
